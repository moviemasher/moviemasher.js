      "icon": "/user-media/test/eisen/256x144x1/0.jpg",
      "type": "image",
      "group": "image"
    },
    {
      "label": "Color",
      "type": "theme",
      "id": "com.moviemasher.theme.color",
      "properties": {
        "color": {
          "type": "rgb",
          "value": "#FFFF00"
        }
      },
      "filters": [
        {
          "id": "color"
        }
      ],
      "html": "/angular-moviemasher/app/module/color/theme.html"
    },
    {
      "label": "Scale",
      "type": "scaler",
      "id": "com.moviemasher.scaler.scale",
      "properties": {
        "scale": {
          "type": "size1",
          "value": 1
        }
      },
      "filters": [
        {
          "id": "scale",
          "parameters": [
            {
              "name": "width",
              "value": "scale * mm_input_width * mm_max(mm_width / mm_input_width, mm_height / mm_input_height)"
            },
            {
              "name": "height",
              "value": "scale * mm_input_height * mm_max(mm_width / mm_input_width, mm_height / mm_input_height)"
            }
          ]
        },
        {
          "id": "setsar",
          "parameters": [
            {
              "name": "sar",
              "value": "1"
            },
            {
              "name": "max",
              "value": "1"
            }
          ]
        }
      ],
      "html": "/angular-moviemasher/app/module/scale/scaler.html"
    },
    {
      "id": "logo",
      "label": "mm-overlay.png",
      "source": "/user-media/test/logo/original.png",
      "url": "/user-media/test/logo/256x144x1/0.png",
      "icon": "/user-media/test/logo/256x144x1/0.png",
      "type": "image",
      "group": "image"
    },
    {
      "label": "Constrained",
      "type": "merger",
      "id": "com.moviemasher.merger.constrained",
      "properties": {
        "left": {
          "type": "pixel",
          "value": 0
        },
        "top": {
          "type": "pixel",
          "value": 0
        }
      },
      "filters": [
        {
          "id": "overlay",
          "parameters": [
            {
              "name": "x",
              "value": "left*(mm_width-overlay_w)"
            },
            {
              "name": "y",
              "value": "top*(mm_height-overlay_h)"
            }
          ]
        }
      ],
      "html": "/angular-moviemasher/app/module/overlay/merger.html"
    },
    {
      "label": "Center",
      "id": "com.moviemasher.merger.center",
      "filters": [
        {
          "id": "overlay",
          "parameters": [
            {
              "name": "x",
              "value": "floor((mm_width - overlay_w) / 2)"
            },
            {
              "name": "y",
              "value": "floor((mm_height - overlay_h) / 2)"
            }
          ]
        }
      ],
      "html": "/angular-moviemasher/app/module/overlay/merger.html",
      "type": "merger"
    }
  ],
