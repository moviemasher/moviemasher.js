{
  "name": "@moviemasher/build-packages",
  "description": "Builds Movie Masher core packages",
  "scripts": {
    "example-express": "npm run build -w @moviemasher/example-express",
    "lib-client": "npm run build -w @moviemasher/lib-client",
    "lib-server": "npm run build -w @moviemasher/lib-server",
    "lib-shared": "npm run build -w @moviemasher/lib-shared",
    "lib": "run-s lib-shared lib-client lib-server",
    "runtime-client": "npm run build -w @moviemasher/runtime-client",
    "runtime-server": "npm run build -w @moviemasher/runtime-server",
    "runtime-shared": "npm run build -w @moviemasher/runtime-shared",
    "runtime": "run-s runtime-shared runtime-client runtime-server",
    "example": "run-s example-express",
    "build": "run-s runtime lib"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.3",
    "npm-run-all": "^4.1.5",
    "rollup": "3.28.0",
    "terser": "5.19.2",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "@types/node": "20.5.1",
    "typescript": "5.1.3",
    "rollup-plugin-typescript2": "0.34.1",
    "esbuild": "0.18.4"
  },
  "bugs": {
    "url": "https://github.com/moviemasher/moviemasher.js/issues"
  },
  "homepage": "https://moviemasher.com",
  "author": {
    "name": "Movie Masher",
    "email": "support@moviemasher.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/moviemasher/moviemasher.js.git"
  },
  "version": "5.1.2"
}