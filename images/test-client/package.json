{
  "name": "@moviemasher/test-client",
  "description": "Runs all tests",
  "scripts": {
    "test-component": "web-test-runner",
    "start": "web-dev-server --node-resolve --open --watch",
    "test-core-client": "node --loader tsx --test ../../packages/lib/shared/src/Client/**/*.test.ts ../../packages/lib/shared/src/Client/*.test.ts",
    "test-runtime-client": "node --loader tsx --test ../../packages/runtime/client/src/**/**/*.test.ts ../../packages/runtime/client/src/**/*.test.ts",
