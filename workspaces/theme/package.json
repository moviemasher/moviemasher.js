{
  "name": "@moviemasher/theme",
  "version": "5.1.2",
  "description": "Movie Masher Default Theme",

  "module": "./esm/theme-default.mjs",
  "main": "./umd/theme-default.js",
  "types": "./esm/theme-default.d.mts",
  "files": ["esm", "umd"],

  "license": "MIT",


  "devDependencies": {
    "@types/node": "18.13.0",
