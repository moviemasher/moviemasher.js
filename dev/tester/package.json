{
  "name": "tester",
  "version": "5.1.0",
  "description": "Movie Masher React Example",
  "author": {
    "name": "Movie Masher",
    "email": "support@moviemasher.com"
  },
  "license": "MPL-2.0",
  "main": "dist/test/index.js",
  "source": "src/app.tsx",
  "scripts": {
    "build-client": "rollup --config rollup.config.js",
    "build": "run-s clean copy build-server build-client",
    "build-server": "tsc --project ./server.tsconfig.json",
    "start": "pm2 start ./dist/server.js",
    "stop": "pm2 delete ./dist/server.js",
    "logs": "pm2 logs ./dist/server.js",
    "clean-create-img": "mkdir -p dist/img",
    "clean-create-shared": "mkdir -p dist/shared",
    "clean-create-test": "mkdir -p dist/test",
    "clean-delete": "rimraf dist",
    "clean": "run-s clean-delete clean-create-img clean-create-shared clean-create-test",
    "copy-html": "cp ../../dev/index.html dist/test",
    "copy-icon": "cp ../../dev/img/favicon.ico dist",
    "copy-img": "cp -R ../../dev/img dist",
    "copy-shared": "cp -R ../../dev/shared dist",
    "copy": "run-s copy-icon copy-shared copy-img copy-html",
    "jest": "jest --config jest.config.js",
    "test": "run-s start \"jest {1}\" --"
  },
  "dependencies": {
    "@moviemasher/client-react": "5.1.0"
  }
}
