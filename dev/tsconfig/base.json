{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "alwaysStrict": true,
    "checkJs": false,
    "declaration": false,
    "declarationMap": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["DOM", "ESNext", "ES2022", "ES2021", "ES2020", "ES2019", "ES2018", "ES2017", "ES2016", "ES2015", "ES5", "ES6"],

    "moduleResolution": "Bundler",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "resolveJsonModule": false,
    "skipLibCheck": false,
    "sourceMap": true,
    "strict": true,
    "module": "ES2022",
    "target": "ES2022"
  }
}
