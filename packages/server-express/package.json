{
  "name": "@moviemasher/server-express",
  "version": "5.1.2",
  "description": "Movie Masher Server Plugin implemented in ExpressJS - version 5.1.2",
  "main": "esm/server-express.js",
  "types": "esm/server-express.d.ts",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Movie Masher",
    "email": "support@moviemasher.com"
  },
  "bugs": {
    "url": "https://github.com/moviemasher/moviemasher.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moviemasher/moviemasher.js.git"
  },
  "homepage": "https://moviemasher.com",
  "scripts": {
    "build-esm": "esbuild src/index.ts --packages=external --bundle --tsconfig=dev/tsconfig.json --platform=neutral --outfile=esm/server-express.js",
    "build-esm-min": "esbuild src/index.ts --sourcemap --packages=external --bundle --tsconfig=dev/tsconfig.json --platform=neutral --outfile=esm/server-express.min.js --minify",
    "build-cjs-min": "rollup --config dev/rollup/cjs.min.config.js",
    "build-cjs": "rollup --config dev/rollup/cjs.config.js",
    "copy-types": "cp cjs/server-express.d.ts esm/server-express.d.ts",
    "build": "run-s build-esm build-esm-min build-cjs build-cjs-min copy-types"
  },
  "peerDependencies": {
    "@bitjourney/uuid-v4": "^1.0.1",

    "@moviemasher/moviemasher.js": "5.1.2",
    "@moviemasher/server-core": "5.1.2",

    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-basic-auth": "^1.2.1",
    "multer": "^1.4.5-lts.1",
    
    "sqlite": "^4.0.25",
    "sqlite3": "^5.0.8",
    
    "knex": "2.4.0",
    

    "tar": "^6.1.11"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/md5": "^2.3.2",
    "@types/multer": "^1.4.7",
    "@types/node": "18.13.0",
    "@types/node-media-server": "^2.3.3"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "server-express",
    "tsconfig": "./dev/tsconfig.json"
  }
}
