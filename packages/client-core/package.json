{
  "name": "@moviemasher/client-core",
  "version": "5.1.2",
  "description": "Movie Masher Core Client - version 5.1.2",
  "main": "umd/client-core.js",
  "module": "esm/client-core.js",
  "type": "module",
  "types": "esm/client-core.d.ts",
  "license": "MIT",
  "author": {
    "name": "Movie Masher",
    "email": "support@moviemasher.com"
  },
  "bugs": {
    "url": "https://github.com/moviemasher/moviemasher.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moviemasher/moviemasher.js.git"
  },
  "homepage": "https://moviemasher.com",
  "scripts": {
    "build-esm": "esbuild src/index.ts --sourcemap --packages=external --bundle --platform=neutral --outfile=esm/client-core.js",
    "build-esm-min": "esbuild src/index.ts --packages=external --bundle --platform=neutral --outfile=esm/client-core.min.js --minify",
    "build-umd-min": "rollup --config dev/rollup/umd.min.config.js",
    "build-umd": "rollup --config dev/rollup/umd.config.js",
    "copy-types": "cp umd/client-core.d.ts esm/client-core.d.ts",
    "build": "run-s build-esm build-esm-min build-umd build-umd-min copy-types"
  },
  "peerDependencies": {
    "@moviemasher/moviemasher.js": "5.1.2"
  },
  "devDependencies": {
    "@types/css-font-loading-module": "^0.0.7"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "client-core",
    "tsconfig": "./tsconfig.json"
  }
}
