{
  "name": "@moviemasher/server-core",
  "version": "5.1.2",
  "description": "Movie Masher Server Core - version 5.1.2",
  "types": "esm/server-core.d.ts",
  "main": "esm/server-core.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Movie Masher",
    "email": "support@moviemasher.com"
  },
  "bugs": {
    "url": "https://github.com/moviemasher/moviemasher.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moviemasher/moviemasher.js.git"
  },
  "homepage": "https://moviemasher.com",
  "scripts": {
    "build-cjs-min": "rollup --config dev/rollup/cjs.min.config.js",
    "build-cjs": "rollup --config dev/rollup/cjs.config.js",
    "build-esm-min": "esbuild src/index.ts --packages=external --bundle --platform=neutral --outfile=esm/server-core.min.js --minify",
    "build-esm": "esbuild src/index.ts --sourcemap --packages=external --bundle --platform=neutral --outfile=esm/server-core.js",
    "copy-types": "cp cjs/server-core.d.ts esm/server-core.d.ts",
    "build": "run-s build-esm build-esm-min build-cjs build-cjs-min copy-types"
  },
  "peerDependencies": {
    "@moviemasher/moviemasher.js": "5.1.2",
    "fluent-ffmpeg": "^2.1.2",
    "ttf2woff2": "5.0.0",
    "child_process": "^1.0.2"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/node": "18.11.18"
  }
}
