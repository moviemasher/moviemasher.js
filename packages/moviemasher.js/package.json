{
  "name": "@moviemasher/moviemasher.js",
  "version": "5.1.2",
  "description": "JavaScript video editor, encoder, switcher - version 5.1.2",
  "main": "esm/moviemasher.js",
  "browser": "umd/moviemsher.js",
  "types": "types/moviemasher.d.ts",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Movie Masher",
    "email": "support@moviemasher.com"
  },
  "bugs": {
    "url": "https://github.com/moviemasher/moviemasher.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moviemasher/moviemasher.js.git"
  },
  "homepage": "https://moviemasher.com",
  "keywords": [
    "video editor",
    "audio editor",
    "video streaming",
    "svg",
    "composite"
  ],
  "scripts": {
    "build-esm": "esbuild src/index.ts --sourcemap --bundle --platform=neutral --outfile=esm/moviemasher.js",
    "build-esm-min": "esbuild src/index.ts --sourcemap --bundle --platform=neutral --outfile=esm/moviemasher.min.js --minify",
    "build-umd-min": "rollup --config dev/rollup/umd.min.config.js",
    "build-umd": "rollup --config dev/rollup/umd.config.js",
    "build-min": "run-s build-esm-min build-umd-min",
    "copy-types": "mkdir -p types && mv -f umd/*.d.* types/",
    "build": "run-s build-esm build-esm-min build-umd build-umd-min copy-types"
  },
  "devDependencies": {
    "typescript": "4.9.5"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "dev/tsdoc/index.md",
    "displayName": "core"
  }
}

